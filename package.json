{
  "name": "picture-processor",
  "version": "0.1.0",
  "description": "Process our digital pictures for archive",
  "main": "index.js",
  "scripts": {
    "start": "babel-node ./src/index.js",
    "movetoserver": "npm start -- -r --movepictures \"/Volumes/photos/ByDate\" --movemovies \"/Volumes/photos/ByDate/Videos\" --moveothers \"/Volumes/photos/ByDate/Unknown\"",
    "copytoserver": "npm start -- -r --copypictures \"/Volumes/photos/ByDate\" --copymovies \"/Volumes/photos/ByDate/Videos\" --copyothers \"/Volumes/photos/ByDate/Unknown\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jeffhandley/picture-processor.git"
  },
  "author": "Jeff Handley",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jeffhandley/picture-processor/issues"
  },
  "homepage": "https://github.com/jeffhandley/picture-processor#readme",
  "dependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "exif": "^0.6.0",
    "exif-date": "^2.0.1",
    "fs-extra": "^3.0.0",
    "mkdirp": "^0.5.1",
    "moment": "^2.18.1",
    "yargs": "^7.1.0"
  }
}
